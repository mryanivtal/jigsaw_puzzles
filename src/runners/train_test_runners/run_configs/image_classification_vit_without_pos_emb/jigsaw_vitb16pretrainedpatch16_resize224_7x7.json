{
  "flow": {
    "run_train": true,
    "run_test": true,
    "_start_from_lt_checkpoint": "D:\\docs\\Study\\DSML_IDC\\Final project\\project\\outputs\\model_7x7.ckpt"
  },

  "trainer": {
    "run_name": "jigsaw_vitb16pretrained_patch16_resize224_lr6e3_batch64_7x7",
    "task": "bin_class",
    "add_timestamp_to_out_dir": true,
    "max_epochs": 200,
    "batch_size": 64,
    "check_val_every_n_epoch": 1,
    "early_stop_patience": 40,
    "num_workers": 2
  },

  "dataset": {
    "short_debug_run": false,
    "dataset_type": "jigsaw",
    "target_type": "probability",
    "train_val_split_ratio": 0.8,
    "transforms": {
      "resize_y": 224,
      "resize_x": 224,
      "random_erasing": false
    },

    "scrambler": {
      "mode": "different_for_all_samples",
      "parts_x": 7,
      "parts_y": 7,
      "permutation_type": "random"
    }
  },

  "model": {
    "name": "vit_b16_224_without_pos_emb",
    "out_features": 1,
    "pretrained": true,
    "inference_normalizer": "sigmoid"
  },

  "optimizer": {
    "name": "adam",
    "lr": 6e-3
  },

  "loss": {
    "name": "bce_loss"
  },

  "sample_saver": {
    "name": "plain",
    "num_samples_to_save": 6
  }
}